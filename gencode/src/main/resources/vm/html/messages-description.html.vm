#macro (rowColor $rowNum)
#if ($rowNum % 2 == 0)
rowColor##
#else
altColor##
#end
#end
##
#macro (label $field)
#if ($field.label.number == 1)
optional
#elseif ($field.label.number == 2)
<strong>required</strong>
#else
repeated
#end
#end
##
#macro (type $field)
#if ($util.isBasicType($field))
${util.getTypeName($field)}
#else
#set ($baseName= $util.getBaseName(${field.typeName}))
<a href="messages-description.html#${baseName}">${baseName}</a>
#end
#end
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- NewPage -->
<html lang="zh">
<head>
    <!-- Generated by pbrpcc ${createTime} -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Messages Description</title>
    <link rel="stylesheet" type="text/css" href="stylesheet.css" title="Style">
</head>
<body>
<h1 title="Messages Description" class="bar"><strong>Messages Description</strong></h1>
#set ($num = 0)
#foreach ($message in $messages)
#if ($num > 0)
<hr />
#end
<A name="${message.descriptor.name}"><!-- --></A>
<div class="header">
    <div class="subTitle">${package}</div>
    <h2 title="${message.descriptor.name}" class="title">${message.descriptor.name}</h2>
</div>
<div class="contentContainer">
    <div class="description">
        <ul class="blockList">
            <li class="blockList">
                <div class="block">${message.comments}
                </div>
            </li>
        </ul>
    </div>
    <ul class="blockList">
            <li class="blockList">
                <table class="packageSummary" border="0" cellpadding="3" cellspacing="0"
                       summary="Message Detail Description">
                    <caption><span>Class Summary</span><span class="tabEnd">&nbsp;</span></caption>
                    <tr>
                        <th class="colFirst" scope="col">Name</th>
                        <th class="colOne" scope="col">Label</th>
                        <th class="colOne" scope="col">Type</th>
                        <th class="colOne" scope="col">Number</th>
                        <th class="colLast" scope="col">Description</th>
                    </tr>
                    <tbody>
#set ($rowNum = 0)
#foreach ($field in $message.children)
                    <tr class="#rowColor($rowNum)">
                        <td class="colFirst">
                            ${field.descriptor.name}
                        </td>
                        <td class="colOne">
                            #label(${field.descriptor})
                        </td>
                        <td class="colOne">
                            #type($field.descriptor)
                        </td>
                        <td class="colOne">
                            ${field.descriptor.number}
                        </td>
                        <td class="colLast">
                            <div class="block">${field.comments}</div>
                        </td>
                    </tr>
#set ($rowNum = $rowNum + 1)
#end
                    </tbody>
                </table>
            </li>
    </ul>
</div>


#set ($num = $num + 1)
#end
</body
</html>