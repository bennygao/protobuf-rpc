<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- NewPage -->
<html lang="zh">
<head>
    <!-- Generated by pbrpcc ${createTime} -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Services Description</title>
    <link rel="stylesheet" type="text/css" href="stylesheet.css" title="Style">
</head>
<body>
<h1 title="Services Description" class="bar"><strong>Services Description</strong></h1>
#set ($num = 0)
#foreach ($service in $services)
#if ($num > 0)
<hr />
#end
<A name="${service.descriptor.name}"><!-- --></A>
<div class="header">
    <div class="subTitle">${package}</div>
    <h2 title="${service.descriptor.name}" class="title">${service.descriptor.name}</h2>
</div>
<div class="contentContainer">
    <div class="description">
        <ul class="blockList">
            <li class="blockList">
                <div class="block">${service.comments}
                </div>
            </li>
        </ul>
    </div>
    <div class="details">
        <ul class="blockList">
            <li class="blockList">
                <!-- ============ METHOD DETAIL ========== -->
                <ul class="blockList">
                    <li class="blockList">
                        <h3>Method Detail</h3>
                        <a name="getDocumentProperties()"><!--   --></a>
                        <ul class="blockList">
#foreach ($method in $service.children)
                            <a name="${service.descriptor.name}-${method.descriptor.name}"><!--   --></a>
                            <li class="blockList">
                                <h4>${method.descriptor.name}</h4>
                                #methodPrototype($method.descriptor)
                                <div class="block">
                                    ${method.comments}
                                </div>
                            </li>
#end
                        </ul>
                    </li>
                </ul>
            </li>
        </ul>
    </div>
</div>
#set ($num = $num + 1)
#end
</body>
</html>