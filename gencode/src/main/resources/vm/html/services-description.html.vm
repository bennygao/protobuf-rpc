#macro (methodPrototype $method)
#set ($outputType = ${util.getBaseName($method.outputType)})
#set ($inputType = ${util.getBaseName($method.inputType)})
<pre><code><a href="messages-description.html#${outputType}">${outputType}</a>&nbsp;&nbsp;${method.name}(<a href="messages-description.html#${inputType}">${inputType}</a>)</code></pre>
#end
